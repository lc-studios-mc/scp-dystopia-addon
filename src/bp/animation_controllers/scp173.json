{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.scp173.blink": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": ["anim.blink_timer.enter"],
					"transitions": [{ "blink": "q.property('scpdt:is_blink_phase')" }]
				},
				"blink": {
					"animations": ["anim.blink_timer.exit"],
					"on_entry": ["/camera @a[m=!c,r=24] fade time 0.1 0.9 0.3 color 0 0 0"],
					"transitions": [{ "default": "!q.property('scpdt:is_blink_phase')" }]
				}
			}
		}
	}
}
