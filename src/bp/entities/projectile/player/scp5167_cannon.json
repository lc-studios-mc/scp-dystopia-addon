{
	"format_version": "1.12.0",
	"minecraft:entity": {
		"description": {
			"identifier": "lc:dt_scp5167_cannon_ball",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:snowball"
		},
		"component_groups": {
			"minecraft:exploding": {
				"minecraft:explode": {
					"fuseLength": 0,
					"fuseLit": true,
					"power": 1.0,
					"causesFire": false,
					"fire_affected_by_griefing": true,
					"destroy_affected_by_griefing": true
				}
			},
			"despawning": {
				"minecraft:despawn": {}
			}
		},
		"components": {
			"minecraft:type_family": {
				"family": ["bullet", "projectile"]
			},
			"minecraft:collision_box": {
				"width": 0.25,
				"height": 0.25
			},
			"minecraft:despawn": {
				"filters": {
					"all_of": [
						{
							"any_of": [
								{
									"test": "is_family",
									"subject": "self",
									"value": "despawer_despawning"
								}
							]
						}
					]
				}
			},
			"minecraft:projectile": {
				"onHit": {
					"impact_damage": {
						"damage": 6,
						"knockback": true,
						"semirandomdiffdamage": false,
						"destroyonhit": false
					},
					"definition_event": {
						"affectProjectile": true,
						"event_trigger": {
							"event": "minecraft:explode",
							"target": "self"
						}
					},
					"mob_effect": {
						"effect": "wither",
						"durationeasy": 0,
						"durationnormal": 10,
						"durationhard": 20,
						"amplifier": 2
					}
				},
				"power": 1.5,
				"gravity": 0.07,
				"uncertaintyBase": 3.0,
				"uncertaintyMultiplier": 1.0,
				"anchor": 1,
				"should_bounce": true,
				"offset": [0, -0.1, 0],
				"reflectOnHurt": true,
				"catch_fire": false
			},
			"minecraft:physics": {},
			"minecraft:timer": {
				"looping": false,
				"time": 7.5,
				"time_down_event": {
					"event": "minecraft:start_despawn"
				}
			},
			"minecraft:conditional_bandwidth_optimization": {
				"default_values": {
					"max_optimized_distance": 80.0,
					"max_dropped_ticks": 7,
					"use_motion_prediction_hints": true
				}
			}
		},
		"events": {
			"minecraft:explode": {
				"add": {
					"component_groups": ["minecraft:exploding"]
				}
			},
			"minecraft:start_despawn": {
				"add": {
					"component_groups": ["despawning"]
				}
			}
		}
	}
}
