{
	"format_version": "1.12.0",
	"minecraft:entity": {
		"description": {
			"identifier": "lc:dt_rpg_rocket",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:snowball"
		},
		"component_groups": {
			"minecraft:exploding": {
				"minecraft:explode": {
					"fuseLength": 0,
					"fuseLit": true,
					"power": 6.0,
					"causesFire": true,
					"fire_affected_by_griefing": true,
					"destroy_affected_by_griefing": true
				}
			},
			"despawning": {
				"minecraft:despawn": {}
			}
		},
		"components": {
			"minecraft:type_family": {
				"family": ["rocket", "projectile"]
			},
			"minecraft:collision_box": {
				"width": 0.4,
				"height": 0.4
			},
			"minecraft:despawn": {
				"filters": {
					"all_of": [
						{
							"any_of": [
								{
									"test": "is_family",
									"subject": "self",
									"value": "despawer_despawning"
								}
							]
						}
					]
				}
			},
			"minecraft:projectile": {
				"onHit": {
					"definition_event": {
						"affectProjectile": true,
						"eventTrigger": {
							"event": "minecraft:explode",
							"target": "self"
						}
					}
				},
				"hit_sound": "gun.hit",
				"power": 1.5,
				"gravity": 0.06,
				"inertia": 1.2,
				"uncertaintyBase": 3,
				"uncertaintyMultiplier": 2,
				"anchor": 1,
				"offset": [0, 0, 0],
				"reflectOnHurt": false,
				"catch_fire": true
			},
			"minecraft:physics": {},
			"minecraft:timer": {
				"looping": false,
				"time": 3,
				"time_down_event": {
					"event": "minecraft:explode"
				}
			},
			"minecraft:conditional_bandwidth_optimization": {
				"default_values": {
					"max_optimized_distance": 80.0,
					"max_dropped_ticks": 7,
					"use_motion_prediction_hints": true
				}
			}
		},
		"events": {
			"minecraft:explode": {
				"add": {
					"component_groups": ["minecraft:exploding"]
				}
			},
			"minecraft:start_despawn": {
				"add": {
					"component_groups": ["despawning"]
				}
			}
		}
	}
}
