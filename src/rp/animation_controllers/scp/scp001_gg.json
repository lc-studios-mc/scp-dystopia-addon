{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.dt_scp001_gg.anim_cont": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": ["general"],
					"transitions": [
						{
							"attacking": "query.is_delayed_attacking == 1"
						}
					],
					"blend_transition": 1.0
				},
				"attacking": {
					"animations": ["attack"],
					"transitions": [
						{
							"default": "query.is_delayed_attacking == 0"
						}
					],
					"blend_transition": 1.0
				}
			}
		}
	}
}
