{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.dt_scp_redacted.humanoid.anim_cont": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": ["humanoid_general", "humanoid_move_arms", "humanoid_move_legs"],
					"transitions": [
						{
							"attacking": "query.is_delayed_attacking == 1"
						}
					],
					"blend_transition": 0.3
				},
				"attacking": {
					"animations": ["humanoid_attack", "humanoid_move_legs"],
					"transitions": [
						{
							"default": "query.all_animations_finished"
						}
					],
					"blend_transition": 0.3
				}
			}
		}
	}
}
