		"controller.animation.item.dt_mp5k.reload": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"empty": "(query.get_equipped_item_name(1) == 'dt_mp5_mag')"
						}
					]
				},
				"empty": {
					"animations": [
						{
							"empty": "c.is_first_person"
						}
					],
					"transitions": [
						{
							"with_ammo": "(query.get_equipped_item_name(1) == 'dt_mp5_mag_empty')"
						},
						{
							"default": "(query.get_equipped_item_name(1) != 'dt_mp5_mag')"
						}
					]
				},
				"with_ammo": {
					"animations": [
						{
							"reloading": "c.is_first_person"
						}
					],
					"transitions": [
						{
							"default": "query.all_animations_finished"
						}
					]
				}
			}
		},
